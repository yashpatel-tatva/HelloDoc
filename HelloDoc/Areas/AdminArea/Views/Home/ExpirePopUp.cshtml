<script>
    Swal.fire({
        imageUrl: "/res/sessionclock.png",
        title: "Your Session has Expired",
        text: "Please Login Again",
        width: 900,
        heightAuto : false,
        showCancelButton: false,
        confirmButtonText: "Log In", 
        backdrop: "radial-gradient(circle at 12.3% 19.3%, rgb(85, 88, 218) 0%, rgb(95, 209, 249) 100.2%)",
        background: "white",
        allowOutsideClick: false,
        allowEscapeKey: false,
        allowEnterKey: false,
        showCancelButton: false,
    }).then((result) => {
        if (result.isConfirmed) {
            window.location.href = "/AdminArea/Home/AdminLogin";
        }
    });
</script>