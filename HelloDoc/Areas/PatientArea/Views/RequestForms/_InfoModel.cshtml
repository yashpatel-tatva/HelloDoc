<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css">
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script>
    Swal.fire({
        title: "<strong>Information</strong>",
        timer: 10000,
        timerProgressBar: true,
        icon: "info",
        html: `
            When Submitting the request, you must provide the correct contact information for the patient or the responsibly party. Failure to provide the correct email and phone number will delay the service or be declined.
      `,
        showCloseButton: false,
        focusConfirm: false,
        confirmButtonText: `
        <i class="fa fa-thumbs-up"></i> Ok!
      `,
        willOpen: () => {
            var ss = document.createElement('link');
            ss.rel = "stylesheet";
            if (sessionStorage.getItem('flag')) {
                flag = parseInt(sessionStorage.getItem('flag'));
            }
            @{
                var sweetalert = "@sweetalert2";
            }
            if (flag == 0) {
                ss.href = "//cdn.jsdelivr.net/npm/@sweetalert/theme-dark@4/dark.css";
            } else {
                ss.href = "//cdn.jsdelivr.net/npm/@sweetalert/theme-minimal/minimal.css";
            }
            document.head.appendChild(ss);
        }
    });
</script>
